{
  "name": "tailwind-exercises",
  "version": "1.0.5",
  "description": "Environment",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test-unit && npm run test-e2e-cli",
    "test-unit": "jest",
    "test-e2e": "cypress open",
    "test-e2e-report": "cypress run --reporter mochawesome",
    "test-e2e-cli": "cypress run",
    "format": "prettier -w src/**/*.{js,mjs}",
    "lint": "eslint src/**/*.{js,mjs}",
    "lint-fix": "eslint src/**/*.{js,mjs} --cache --fix",
    "prepare": "husky install"
  },
  "author": "Jon Henrik Ã…vitsland",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@eslint/js": "^8.57.0",
    "autoprefixer": "^10.4.19",
    "cypress": "^13.13.1",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-jest": "^28.6.0",
    "globals": "^15.8.0",
    "husky": "^9.1.2",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.7",
    "vite": "^5.3.5"
  },
  "lint-staged": {
    "*.{js,mjs}": "prettier --write",
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix"
  }
}
